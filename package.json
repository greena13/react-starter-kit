{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "description": "React Starter Kit: React, Babel, Reflux, React Router, Koa, Webpack, Hot Reloading",
  "author": "Aleck Greenham",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "watch": "concurrently --kill-others \"npm run watch-client\" \"npm run watch-server-start\" \"npm run watch-server\"",
    "watch-client": "BABEL_ENV=development-client babel-node ./src/webpackDevServer.js",
    "watch-server-start": "node node_modules/just-wait -t 120 --pattern \"build/scripts/server.js\" && node ./build/scripts/server.js",
    "watch-server": "node node_modules/just-wait -t 120 --pattern \"build/assets.js\" && BABEL_ENV=development-server webpack --watch --verbose --colors --display-error-details --config config/webpack.server.development.babel.js",
    "build-server": "NODE_ENV=production BABEL_ENV=production-server webpack --colors --display-error-details --config config/webpack.server.production.babel.js",
    "build-client": "NODE_ENV=production BABEL_ENV=production-client webpack --colors --display-error-details --config config/webpack.client.production.babel.js",
    "build": "npm run build-client && npm run build-server",
    "stats": "NODE_ENV=production BABEL_ENV=production-client webpack --profile --json --config config/webpack.client.config.js > stats.json"
  },
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "private": true,
  "dependencies": {
    "fastclick": "^1.0.6",
    "history": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^1.2.0",
    "koa-pug": "^3.0.0-0",
    "less": "^2.7.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.23.0",
    "clean-webpack-plugin": "^0.1.9",
    "compression-webpack-plugin": "^0.3.1",
    "concurrently": "^3.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.11",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.3.1",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1"
  }
}
