{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "description": "React Starter Kit: React, Babel, Reflux, React Router, Koa, Webpack, Hot Reloading",
  "author": "Aleck Greenham",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./build/server.js",
    "build-server": "NODE_ENV=production BABEL_ENV=production-server webpack --progress --colors --display-error-details --config config/webpack.server.production.js",
    "build-client": "NODE_ENV=production BABEL_ENV=production-client webpack --progress --colors --display-error-details --config config/webpack.client.production.js",
    "build": "npm run build-client && npm run build-server",
    "build-local": "npm run build-client && npm run build-server",
    "watch-server-start": "node node_modules/just-wait --pattern \"build/server.js\" && npm run start",
    "watch-server": "node node_modules/just-wait --pattern \"build/assets.js\" && BABEL_ENV=development-server webpack --watch --verbose --colors --display-error-details --config config/webpack.server.development.js",
    "watch-client": "BABEL_ENV=development-client babel-node ./src/client.hotreloader.js",
    "watch": "concurrently --kill-others \"npm run watch-client\" \"npm run watch-server-start\" \"npm run watch-server\"",
    "stats": "NODE_ENV=production BABEL_ENV=production-client webpack --profile --json --config config/webpack.client.production.js > stats.json"
  },
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "private": true,
  "dependencies": {
    "concurrently": "^2.0.0",
    "fastclick": "^1.0.6",
    "forever": "^0.15.1",
    "history": "^2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "just-wait": "^1.0.5",
    "koa": "^1.2.0",
    "koa-pug": "^3.0.0-0",
    "less": "^2.7.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
